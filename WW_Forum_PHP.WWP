  internal_properties : CAAAAAgAAAA7D83nWa42F2RrmOaG33G7l2win0MHW+9cjYaOMPTCXjWKeeXZZyyv8o2st6Q1HBMdH2gwAEUqnWQxgdenTLkL7FDzisXB2QShge2/Wf5bQeYXJgEZXvpKCxl27FAlvnUC9nvvlqaO7AU8jO7M1nuIlhvf7ZmIENRb0Tuy/tbVITnlX/VFv6LGS9G0IaKvkt2ogLOqmg==
  type_code : 0
  p_codes :
   -
     internal_properties : 
     code : |1+
      
      // Configuration du contexte AWP. On stocke les informations de session sur un cookie
      ConfigureAWPContext(ctxCookie)
      
      // Ajout des variables globales dans le contexte
      gnIDUtilisateur is int = -1
      gsPagePrécédente is string = “”
      DeclareAWPContext(gnIDUtilisateur,gsPagePrécédente)
      
      
      // Constantes
      CONSTANT
      	Edit_Nouveau  = “New”
      	Edit_Modifier = “Mod”	
      END
      
      
      
      //
      //
      //Réglette de navigation - Utilisation de l'exemple WW_RegletteAWP
      //
      //
      CONSTANT
      	pBorneMin  = “min”
      	pNbRésultats = “limit”
      END
      
      //
      // Variables nécessaires à la réglette
      //
      gfnFrom		 	is int		// Borne minimale des enregistrements à visualiser
      gfnNbResultat 	is int		// Nombre de résultats à afficher par page
      
      //
      // Récupération des paramètres
      //
      gfnFrom		 	= PageParameter(pBorneMin)		// Récupère la borne minimale
      gfnNbResultat	= PageParameter(pNbRésultats)	// Récupère le nombre de résultats
      
      //
      // Traitement des paramètres
      //
      
      // NOMBRE DE RESULTAT
      // Peut valoir :
      // - 5		: 5 résultats (par défaut)
      // - 10		: 10 résultats
      // - 15		: 15 résultats
      // - 20		: 20 résultats
      // - 0   	: tous les résultats
      
      // Vérifie les bornes
      IF gfnNbResultat <> 9999 THEN
      	// Plus petit que 5 ?
      	IF gfnNbResultat <= 5 THEN gfnNbResultat = 5
      	// Plus grand que 20 ?
      	IF gfnNbResultat >= 20 THEN gfnNbResultat = 20
      	
      	// Si ce n'est pas 5, 10, 15, 20 ?
      	IF modulo(gfnNbResultat, 5) <> 0 THEN
      		// Borne par défaut
      		gfnNbResultat = 5
      	END
      END
      
      // Si la borne minimale n'est pas valide, la borne est à 0
      IF gfnFrom < 0 OR gfnFrom > 9999 THEN 	
      	gfnFrom = 0
      END
      
      
      
      //
      //
      //création d'un administrateur s'il n'y en a pas
      //
      //
      LOCAL
      bNouveau is boolean
      	
      IF HFileExist(Utilisateur) = False THEN
      	bNouveau = True
      END
      
      HCreationIfNotFound(“*”)
      
      IF bnouveau THEN
      CréationSysteme:
      	// Création de l'utilisateur système
      	utilisateur.Administrateur = True
      	Utilisateur.Avatar = “../images/avatar/user.png”
      	utilisateur.Email   = “info@forum-webdev.net”
      	utilisateur.EnLigne = False
      	utilisateur.Login   = <§@00000000000000000004§>
      	utilisateur.MotDePasse = <§@00000000000000000005§>
      	Utilisateur.DateCréation = “19800101000000”
      	utilisateur.Signature  = <§@00000000000000000007§> + CR
      	Utilisateur.TotalMessage = 0
      	HAdd(Utilisateur)
      ELSE
      	HReadSeekFirst(Utilisateur,Login,<§@00000000000000000004§>)	
      	IF NOT HFound(Utilisateur) THEN
      		GOTO CréationSysteme
      	END
      END
     type : 196608
   -
     internal_properties : CAAAAAgAAAChbPWjJhv7izAO4u5LK2sMxBXva7b3kHCucFq/rqJ2verq1Q6tqCt7jqXTTM+Z4//aK980nB6UMYVmMqwPIghPd5kd/MrtaVKagnp4TY7vRWxBQdlVYAD/FBAX52CveMaX6fc597CSG6Ruxw+AjERuOjC3eFrIBKbCRo12jycnv8v7xqI+T88U0v08KK7sJvJuJBjQqLDvyQD1sNs5
     code : |1+
      
      // Global variables
      gnUserID is int = -1
      gsPreviousPage is string = "PAGE_Home"
      gsQueryString is string = ""
      
      
      //If we are not running the test in "Debug the PHP project in simulation mode" mode, connect to the database
      //otherwise, we will be in HyperFileSQL Classic, no need to have a connection code...
      IF InPHPSimulatorMode() = False THEN 
      	
      	//Connection to the database... 
      	gcntConnectionToDatabase is connection
      	
      	//Specify the parameters for connecting to the database 
      	//This can be a MySQL, Oracle or PostGreSQL database
      	//See the online help for more details about how to handle the data in WebDev PHP 
      	gcntConnectionToDatabase..Server = "localhost"
      	gcntConnectionToDatabase..Database = "forumus"
      	gcntConnectionToDatabase..User = "root"
      	gcntConnectionToDatabase..Password = ""
      	gcntConnectionToDatabase..Provider = hNativeAccessMySQL // hNativeAccessMySQL or hNativeAccessOracle or hNativeAccessPostgreSQL
      	
      	//
      	//
      	// To connect as administrator on the site, use the account: "Administrator" with the "demo" password
      	//
      	//
      	
      	
      	// Establish the connection
      	IF NOT HOpenConnection(gcntConnectionToDatabase) THEN
      		EndProgram("Unable to connect to the database - To use this project, the parameters for connecting to the database must be specified in the initialization code of the project.")
      	ELSE
      		HChangeConnection("*",gcntConnectionToDatabase)
      		HCreationIfNotFound("*")
      	END
      END
      
      // Initialize the Administrator account
      InitializeData()
